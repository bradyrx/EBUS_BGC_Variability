@article{cropper_spatial_2014,
	Abstract = {Seasonal coastal upwelling was analyzed along the NW African coastline (11--35$\,^{\circ}$N) from 1981 to 2012. Upwelling magnitudes are calculated by wind speed indices, sea-surface temperature indices and inferred from meteorological station, sea-surface height and vertical water column transport data. A permanent annual upwelling regime is documented across 21--35$\,^{\circ}$N and a seasonal regime across 12--19$\,^{\circ}$N, in accordance with the climatology of previous studies. Upwelling regions were split into three zones: (1) the Mauritania--Senegalese upwelling zone (12--19$\,^{\circ}$N), (2) the strong permanent annual upwelling zone (21--26$\,^{\circ}$N) and (3) the weak permanent upwelling zone (26--35$\,^{\circ}$N). We find compelling evidence in our various indices for the Bakun upwelling intensification hypothesis due to a significant coastal summer wind speed increase, resulting in an increase in upwelling-favorable wind speeds north of 20$\,^{\circ}$N and an increase in downwelling-favorable winds south of 20$\,^{\circ}$N. The North Atlantic Oscillation plays a leading role in modifying interannual variability during the other seasons (autumn--spring), with its influence dominating in winter. The East Atlantic pattern shows a strong correlation with upwelling during spring, while El Ni{\~n}o Southern Oscillation and Atlantic Multi-decadal Oscillation teleconnections were not found. A disagreement between observationally-based wind speed products and reanalysis-derived data is explored. A modification to the Bakun upwelling intensification hypothesis for NW Africa is presented, which accounts for the latitudinal divide in summer wind regimes.},
	Author = {Cropper, Thomas E. and Hanna, Edward and Bigg, Grant R.},
	Doi = {10.1016/j.dsr.2014.01.007},
	File = {cropper_et_al_2014_deep_sea_research_part_i_oceanographic_research_papers.pdf:/Users/ribr5703/Zotero/storage/5Q9BIFGE/cropper_et_al_2014_deep_sea_research_part_i_oceanographic_research_papers.pdf:application/pdf;ScienceDirect Snapshot:/Users/ribr5703/Zotero/storage/8R8YBX4W/S0967063714000181.html:text/html},
