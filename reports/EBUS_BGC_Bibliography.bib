	Issn = {1726-4189},
	Journal = {Biogeosciences},
	Month = sep,
	Number = {17},
	Pages = {5065--5083},
	Title = {Using present-day observations to detect when anthropogenic change forces surface ocean carbonate chemistry outside preindustrial bounds},
