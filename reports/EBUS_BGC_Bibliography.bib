	Keywords = {California Current, 4805 Biogeochemical cycles, processes, and modeling, 4806 Carbon cycling, 4815 Ecosystems, structure, dynamics, and modeling, 9355 Pacific Ocean, air-sea CO2 flux, carbon budget, coastal topography, model resolution, upwelling system},
	Language = {en},
