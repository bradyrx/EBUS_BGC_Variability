	Series = {Eastern {Boundary} {Upwelling} {Ecosystems}: {Integrative} and {Comparative} {Approaches}},
	Title = {Sub-regional ecosystem variability in the {Canary} {Current} upwelling},
	Url = {http://www.sciencedirect.com/science/article/pii/S0079661109001001},
	Urldate = {2017-07-18},
