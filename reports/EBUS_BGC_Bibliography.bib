@article{garcia-reyes_comparison_2017,
	Abstract = {The California Current System (CCS) has two independent seasonal modes of upwelling variability, summer and winter, driven by different atmospheric processes. The variability of upwelling winds during winter is particularly important as strong, episodic events, driven by atmospheric teleconnections with the equatorial Pacific that are active in this season, impact ecological systems along the west coast of North America. Given the importance of upwelling seasonality to ecosystem function, we hypothesize that the Benguela Current System (BCS) shows similar independent seasonal modes of upwelling variability. To test this hypothesis, compare modes of variability between systems, and investigate potential drivers, we use an upwelling index derived from NCEP2 wind data (1979--2014) for the northern, southern, and Agulhas Bank areas of the BCS. In the northern and southern BCS, only one mode of upwelling variability is observed: year-round in the north and during the austral spring and summer (October through April) in the south. The Agulhas Bank region shows two modes of seasonal variability. Based on this 35-year dataset, summer upwelling modes in both the CCS and BCS appear to have similar decadal-scale variability. The other modes of variability (winter mode in the CCS and the non-seasonal second mode in the BCS) are correlated with year-to-year variability in the positioning of regional oceanic high-pressure systems. The leading mode of upwelling variability in the Agulhas Bank region, in the austral summer/fall, is highly correlated with sea level pressure as well as sea surface temperature in the equatorial Pacific, in a spatial and seasonal pattern (boreal winter) resembling the El Ni{\~n}o-Southern Oscillation. Across the CCS, modes of upwelling variability are similar to one another, while modes differ between regions in the BCS. This difference could lead to regional mismatches in favorable ecological conditions. In contrast with the spatially synchronous winter variability influencing the entire CCS ecosystem, substantial regional variation in the BCS may have strong effects on ecosystem functions, especially for species (e.g., small pelagic fish) that migrate between the Agulhas Bank and other areas of the BCS.},
	Author = {Garc{\'\i}a-Reyes, Marisol and Lamont, Tarron and Sydeman, William J. and Black, Bryan A. and Rykaczewski, Ryan R. and Thompson, Sarah Ann and Bograd, Steven J.},
	Doi = {10.1016/j.jmarsys.2017.06.002},
	File = {garc{\'\i}a-reyes_et_al_2017_journal_of_marine_systems.pdf:/Users/ribr5703/Zotero/storage/565JCEPS/garc{\'\i}a-reyes_et_al_2017_journal_of_marine_systems.pdf:application/pdf;ScienceDirect Snapshot:/Users/ribr5703/Zotero/storage/YNJC4BCZ/S0924796317300155.html:text/html},
	Issn = {0924-7963},
	Journal = {Journal of Marine Systems},
	Keywords = {Benguela upwelling system, California upwelling system, Upwelling seasonality, Upwelling variability},
	Month = jun,
	Title = {A comparison of modes of upwelling-favorable wind variability in the {Benguela} and {California} current ecosystems},
