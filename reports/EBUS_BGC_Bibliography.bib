	Journal = {Geophys. Res. Lett.},
	Keywords = {4215 Climate and interannual variability, 4279 Upwelling and convergences, 4516 Eastern boundary currents, 1626 Global climate models, California Current, coastal upwelling, Earth System Model, internal variability, model ensemble},
	Language = {en},
