@article{hales_satellite-based_2012,
	Abstract = {Continental margin carbon cycling is complex, highly variable over a range of space and time scales, and forced by multiple physical and biogeochemical drivers. Predictions of globally significant air--sea CO2 fluxes in these regions have been extrapolated based on very sparse data sets. We present here a method for predicting coastal surface-water pCO2 from remote-sensing data, based on self organizing maps (SOMs) and a nonlinear semi-empirical model of surface water carbonate chemistry. The model used simple empirical relationships between carbonate chemistry (total dissolved carbon dioxide (TCO2) and alkalinity (TAlk)) and satellite data (sea surface temperature (SST) and chlorophyll (Chl)). Surface-water CO2 partial pressure (pCO2) was calculated from the empirically-predicted TCO2 and TAlk. This directly incorporated the inherent nonlinearities of the carbonate system, in a completely mechanistic manner. The model's empirical coefficients were determined for a target study area of the central North American Pacific continental margin (22--50$\,^{\circ}$N, within 370km of the coastline), by optimally reproducing a set of historical observations paired with satellite data. The model-predicted pCO2 agreed with the highly variable observations with a root mean squared (RMS) deviation of {\textless}20μatm, and with a correlation coefficient of {\textgreater}0.8 (r=0.81; r2=0.66). This level of accuracy is a significant improvement relative to that of simpler models that did not resolve the biogeochemical sub-regions or that relied on linear dependences on input parameters. Air--sea fluxes based on these pCO2 predictions and satellite-based wind speed measurements suggest that the region is a ∼14TgCyr−1 sink for atmospheric CO2 over the 1997--2005 period, with an approximately equivalent uncertainty, compared with a ∼0.5TgCyr−1 source predicted by a recent bin-averaging and interpolation-based estimate for the same area.},
	Author = {Hales, Burke and Strutton, Peter G. and Saraceno, Martin and Letelier, Ricardo and Takahashi, Taro and Feely, Richard and Sabine, Christopher and Chavez, Francisco},
	Doi = {10.1016/j.pocean.2012.03.001},
	File = {hales_et_al_2012_progress_in_oceanography.pdf:/Users/ribr5703/Zotero/storage/6Y6LKDZE/hales_et_al_2012_progress_in_oceanography.pdf:application/pdf;ScienceDirect Snapshot:/Users/ribr5703/Zotero/storage/82R4TW24/S0079661112000183.html:text/html},
	Issn = {0079-6611},
	Journal = {Progress in Oceanography},
	Month = sep,
	Pages = {1--15},
	Title = {Satellite-based prediction of {pCO}2 in coastal waters of the eastern {North} {Pacific}},
