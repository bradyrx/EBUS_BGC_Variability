	File = {Snapshot:/Users/ribr5703/Zotero/storage/ATHAGFL8/abstract.html:text/html;sutton_et_al_2014_global_biogeochemical_cycles.pdf:/Users/ribr5703/Zotero/storage/GDSVFJY6/sutton_et_al_2014_global_biogeochemical_cycles.pdf:application/pdf},
	Issn = {1944-9224},
	Journal = {Global Biogeochem. Cycles},
	Keywords = {4215 Climate and interannual variability, climate change, ENSO, 0428 Carbon cycling, 4231 Equatorial oceanography, 4522 ENSO, CO2, El Ni{\~n}o, ocean acidification},
	Language = {en},
	Month = feb,
