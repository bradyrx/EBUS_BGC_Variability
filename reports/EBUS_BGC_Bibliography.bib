@article{alin_robust_2012,
	Abstract = {The California Current System (CCS) is expected to experience the ecological impacts of ocean acidification (OA) earlier than most other ocean regions because coastal upwelling brings old, CO2-rich water relatively close to the surface ocean. Historical inorganic carbon measurements are scarce, so the progression of OA in the CCS is unknown. We used a multiple linear regression approach to generate empirical models using oxygen (O2), temperature (T), salinity (S), and sigma theta (σθ) as proxy variables to reconstruct pH, carbonate saturation states, carbonate ion concentration ([CO32−]), dissolved inorganic carbon (DIC) concentration, and total alkalinity (TA) in the southern CCS. The calibration data included high-quality measurements of carbon, oxygen, and other hydrographic variables, collected during a cruise from British Columbia to Baja California in May--June 2007. All resulting empirical relationships were robust, withr2values {\textgreater}0.92 and low root mean square errors. Estimated and measured carbon chemistry matched very well for independent data sets from the CalCOFI and IMECOCAL programs. Reconstructed CCS pH and saturation states for 2005--2011 reveal a pronounced seasonal cycle and inter-annual variability in the upper water column. Deeper in the water column, conditions are stable throughout the annual cycle, with perennially low pH and saturation states. Over sub-decadal time scales, these empirical models provide a valuable tool for reconstructing carbonate chemistry related to ocean acidification where direct observations are limited. However, progressive increases in anthropogenic CO2 content of southern CCS water masses must be carefully addressed to apply the models over longer time scales.},
	Author = {Alin, Simone R. and Feely, Richard A. and Dickson, Andrew G. and Hern{\'a}ndez-Ay{\'o}n, J. Mart{\'\i}n and Juranek, Lauren W. and Ohman, Mark D. and Goericke, Ralf},
	Doi = {10.1029/2011JC007511},
	File = {alin_et_al_2012_journal_of_geophysical_research_oceans.pdf:/Users/ribr5703/Zotero/storage/CADASAIX/alin_et_al_2012_journal_of_geophysical_research_oceans.pdf:application/pdf;Snapshot:/Users/ribr5703/Zotero/storage/VWTYB2FR/abstract.html:text/html},
	Issn = {2156-2202},
	Journal = {J. Geophys. Res.},
	Keywords = {1635 Oceans, California Current System, ocean acidification, 4805 Biogeochemical cycles, processes, and modeling, 4806 Carbon cycling, 1615 Biogeochemical cycles, processes, and modeling, 4835 Marine inorganic chemistry, pH, aragonite saturation, carbonate chemistry, carbonate saturation},
	Language = {en},
	Month = may,
	Number = {C5},
	Pages = {C05033},
	Title = {Robust empirical relationships for estimating the carbonate system in the southern {California} {Current} {System} and application to {CalCOFI} hydrographic cruise data (2005--2011)},
	Url = {http://onlinelibrary.wiley.com/doi/10.1029/2011JC007511/abstract},
	Urldate = {2017-08-28},
