@article{collins_mean_2000,
	Abstract = {Velocity and hydrographic data collected in the upper kilometer of the water column on 19 bi-monthly cruises along a 225km section off Point Sur, California, from April 1988 to April 1991 are used to describe the mean conditions. The transition between the poleward flowing inshore countercurrents and the equatorward flowing California Current occurred about 100km from the coast. The magnitude of the vertically averaged flow associated with the core of the inshore flow was 4.2cms−1, and its direction, 310$\,^{\circ}$T, was parallel to the topography south of Pt Sur. Maximum flow, 10cms−1, was observed at 100m next to the coast. The associated volume and heat fluxes were 2.9Sv and 0.6×1014W.},
