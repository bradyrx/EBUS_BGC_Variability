	Issn = {1944-9224},
	Journal = {Global Biogeochem. Cycles},
	Keywords = {1635 Oceans, climate change, 1616 Climate variability, 1615 Biogeochemical cycles, processes, and modeling, 3339 Ocean/atmosphere interactions, carbon trends, surface ocean pCO2},
	Language = {en},
