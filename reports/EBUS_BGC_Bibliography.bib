@article{crawford_resonant_2017,
	Abstract = {Low frequency variability of the California Current System (CCS) is investigated using circulation estimates based on a 31-year (1980--2010) sequence of historical analyses of the CCS calculated using the Regional Ocean Modeling System (ROMS) 4-dimensional variational (4D-Var) data assimilation system. The leading 3-dimensional multivariate empirical orthogonal functions (3D EOFs) of the CCS circulation were computed and provide a detailed view of low-frequency circulation variability within the CCS. The 3D EOFs are used as basis functions for a linear inverse model of the circulation, and several Principal Oscillation Patterns (POPs) of the circulation are identified. The leading POPs have periods in the range ∼4−10 years, and shed light on the 3-dimensional time evolving structure associated with low-frequency variability in the circulation. A particular focus here is coastal upwelling. In particular, a POP with a period close to 10 years appears to be preferentially excited as a resonant response to forcing associated with the regional expression of the Pacific Decadal Oscillation, the North Pacific Gyre Oscillation and the El Ni{\~n}o Southern Oscillation.},
	Author = {Crawford, William J. and Moore, Andrew M. and Jacox, Michael G. and Fiechter, J{\'e}r{\^o}me and Neveu, Emilie and Edwards, Christopher A.},
	Doi = {10.1016/j.dsr2.2017.07.016},
	File = {crawford_et_al_2017_deep_sea_research_part_ii_topical_studies_in_oceanography.pdf:/Users/ribr5703/Zotero/storage/LIQJYVZT/crawford_et_al_2017_deep_sea_research_part_ii_topical_studies_in_oceanography.pdf:application/pdf;ScienceDirect Snapshot:/Users/ribr5703/Zotero/storage/A6CXUGU2/S0967064517302898.html:text/html},
	Issn = {0967-0645},
	Journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
	Keywords = {California Current, Linear inverse modeling, North Pacific Gyre Oscillation, Pacific Decadal Oscillation},
	Month = jul,
	Title = {A resonant response of the {California} {Current} circulation to forcing by low frequency climate variability},
	Url = {http://www.sciencedirect.com/science/article/pii/S0967064517302898},
