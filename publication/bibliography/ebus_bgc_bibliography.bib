	File = {ScienceDirect Snapshot:/Users/ribr5703/Zotero/storage/28TRPNZY/S0967064508004311.html:text/html;takahashi_et_al_2009_deep_sea_research_part_ii_topical_studies_in_oceanography.pdf:/Users/ribr5703/Zotero/storage/YHHKDC4N/takahashi_et_al_2009_deep_sea_research_part_ii_topical_studies_in_oceanography.pdf:application/pdf},
	Issn = {0967-0645},
