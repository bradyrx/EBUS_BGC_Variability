	Keywords = {Africa, Climate change, Coastal upwelling, NAO, SST, Wind},
	Month = apr,
	Pages = {94--111},
	Title = {Spatial and temporal seasonal trends in coastal upwelling off {Northwest} {Africa}, 1981--2012},
